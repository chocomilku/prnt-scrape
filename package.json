{
	"name": "prnt-scrape",
	"version": "1.1.3",
	"description": "scraping direct links from prnt.sc links",
	"main": "index.ts",
	"scripts": {
		"start": "ts-node .",
		"version": "genversion --esm --semi --verbose utils/version.ts",
		"build": "npm run version && npx esbuild index.ts --bundle --outfile=build/build.cjs --format=cjs --platform=node",
		"compile": "npx pkg -t node18-linux-x64,node18-win-x64,node18-macos-x64 --out-path dist/ --compress GZip build/build.cjs"
	},
	"author": "chocomilku",
	"license": "ISC",
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"@types/node": "^20.11.17",
		"axios": "^1.6.7",
		"cheerio": "^1.0.0-rc.12",
		"picocolors": "^1.0.0",
		"ts-node": "^10.9.2"
	},
	"devDependencies": {
		"genversion": "^3.2.0"
	}
}
